  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/pagination": "^3.0.0",
    "@types/better-sqlite3": "^7.6.0",
    "better-sqlite3": "^7.4.6",
    "discord.js": "^14.2.0",
