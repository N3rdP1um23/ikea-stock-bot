{
  "name": "ikea-stock-bot",
  "version": "0.0.1",
  "description": "Discord bot used to handle monitoring and alerting of Ikea sock.",
  "main": "main.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "serve": "node build/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/N3rdP1um23/ikea-stock-bot.git"
  },
  "keywords": [
    "ikea",
    "discord",
    "bot",
    "js"
  ],
  "author": "N3rdP1um23",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/N3rdP1um23/ikea-stock-bot/issues"
  },
  "homepage": "https://github.com/N3rdP1um23/ikea-stock-bot#readme",
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "@discordx/pagination": "^2.0.1",
    "discord.js": "^13.6.0",
    "discordx": "^9.4.3",
    "dotenv": "^16.0.0",
    "ikea-availability-checker": "^1.1.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "typescript": "^4.6.3"
